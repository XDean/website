{
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": false,
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "es6",
      "es2016"
    ],
    "typeRoots": [
      "types/deno.d.ts"
    ],
    "plugins": [
      {
        "name": "typescript-deno-plugin",
        "enable": true,
        "importmap": "import_map.json"
      }
    ]
  }
}
